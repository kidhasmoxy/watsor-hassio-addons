{
  "name": "Watsor",
  "version": "1.01",
  "slug": "watsor",
  "description": "Object detection for video surveillance",
  "url": "https://github.com/asmirnou/watsor-haasio-addons/watsor",
  "startup": "services",
  "boot": "auto",
  "user": "root",
  "full_access": "true",
  "ingress": "true",
  "ingress_port": 8080,
  "ports": {
    "8080/tcp": 8080
  },
  "map": [
    "config:ro"
  ],
  "schema": {
    "CONFIG_FILE": "str"
  },
  "options": {
    "CONFIG_FILE": "/config/watsor/config.yaml"
  },
  "devices": [
    "/dev/bus/usb:/dev/bus/usb:rwm",
    "/dev/dri:/dev/dri:rwm"
  ],
  "arch": ["amd64"]
}